{
  "name": "yconnect-cli",
  "version": "1.0.0",
  "description": "CLI for Yahoo! Identity Federation",
  "repository": "git@github.com:tona0516/yconnect-cli.git",
  "author": "tona0516 <tonango.0516@gmail.com>",
  "license": "MIT",
  "scripts": {
    "run": "npm run build && node dist/main.js",
    "build": "run-s build:clean build:tsc",
    "build:clean": "rm -rf dist",
    "build:tsc": "tsc --build",
    "lint": "run-s lint:tsc lint:eslint lint:prettier",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --cache ./src",
    "lint:prettier": "prettier --check ./src",
    "lint-fix": "run-s lint-fix:eslint lint-fix:prettier",
    "lint-fix:eslint": "eslint --cache --fix ./src",
    "lint-fix:prettier": "prettier --write ./src"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "node": "^19.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/commander": "^2.12.2",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/open": "^6.2.1",
    "@types/pino": "^7.0.5",
    "@types/qs": "^6.9.7",
    "axios": "^1.1.3",
    "build-url-ts": "^6.1.3",
    "commander": "^9.4.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "open": "^8.4.0",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "qs": "^6.11.0"
  }
}
